package com.javaprogram;

public class If_else {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		if(1<2) {
			System.out.println(" this condition is true");
		}else {
			System.out.println(" the condition is false");
		}
       
		if(true == false) {
			System.out.println("my statment is correct ");
		}else {
			System.out.println("my statment is incorrect");
		}
		
		int i = 10;
		int j = 20;
		int k = 30;
		if(i<j & j<k) {
			System.out.println("i is largest");
		}else if (j>i & i>k) {
			System.out.println(" j is largest");
	}else {
		System.out.println("k is largest");
		}	
		}

}
	
